{
    "name": "olipayne/guzzle-web-bot-auth-middleware",
    "description": "Guzzle middleware for HTTP Message Signatures (RFC9421) with web-bot-auth.",
    "type": "library",
    "keywords": [
        "guzzle",
        "http",
        "signatures",
        "message-signatures",
        "rfc9421",
        "web-bot-auth",
        "ed25519",
        "middleware",
        "psr-7",
        "psr-15",
        "psr-18"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Payne",
            "email": "olipayne@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "ext-sodium": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Olipayne\\GuzzleWebBotAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Olipayne\\GuzzleWebBotAuth\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --configuration phpunit.xml.dist",
        "test:coverage": "phpunit --configuration phpunit.xml.dist --coverage-text"
    }
}
